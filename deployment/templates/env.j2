APP_NAME=Laravel
APP_ENV=production
APP_DEBUG=false
APP_URL=http://localhost

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=gateway
DB_USERNAME=root
DB_PASSWORD={{ db_password }}

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

QUEUE_CONNECTION=sqs
SQS_KEY={{ sqs_key }}
SQS_SECRET={{ sqs_secret }}
SQS_REGION={{ sqs_region }}
SQS_QUEUE={{ sqs_name }}
SQS_PREFIX={{ sqs_prefix }}

SENTRY_DSN={{ sentry_dsn }}
SENTRY_TRACES_SAMPLE_RATE=1

OBADA_NODE_USER={{ obada_node_user }}
OBADA_NODE_PASSWORD={{ obada_node_password }}
